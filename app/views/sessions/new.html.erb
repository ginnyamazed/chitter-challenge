<h1>Log In</h1>
<%= form_with url: login_path do |f| %>

  <div class="mb-3">
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </div>

  <div class="mb-3">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div>

  <div class="mb-3">
    <%= f.submit "Login", class: "btn btn-primary" %>
  </div>
<% end %>